<h2>Sign Up</h2>

<form action="<%= users_url %>" method="post">
  <%= auth_token %>

  <label>Name
    <input name="user[name]"
            type="text"
            value="<%= @user.name %>"></label>

  <label>Password
    <input name="user[password]"
           type="password"></label>

  <input type="submit" value="Sign Up">

</form>
