<h2>Sign In</h2>

<form action="<%= session_url %>" method="post">
  <%= auth_token %>

  <label>Name
    <input name="user[name]"
            type="text"
            value=""></label>

  <label>Password
    <input name="user[password]"
           type="password"></label>

  <input type="submit" value="Sign In">

</form>
